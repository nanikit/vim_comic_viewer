{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "denopack",
      "type": "shell",
      "command": "deno run --unstable --allow-all https://raw.githubusercontent.com/jeiea/denopack/deno-1.16.4/cli.ts --config build_src/denopack_config.ts -o vim_comic_viewer.user.js --input ${workspaceFolder}/src/mod.tsx",
      "group": "build",
      "problemMatcher": []
    },
    {
      "label": "denopack watch",
      "type": "shell",
      "command": "deno run --unstable --allow-all https://raw.githubusercontent.com/jeiea/denopack/deno-1.16.4/cli.ts --config build_src/denopack_config.ts -o vim_comic_viewer.user.js --input ${workspaceFolder}/src/mod.tsx --watch src",
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": []
    }
  ]
}
