{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build",
      "type": "deno",
      "command": "task",
      "args": ["build"],
      "group": "none",
      "problemMatcher": "$deno"
    },
    {
      "label": "watch",
      "type": "deno",
      "command": "task",
      "args": ["watch"],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": "$deno"
    }
  ]
}
